,KAAA,CAAA,OAAA,GAAO,CAAP,GAAO,KAAP,GAAO,OAAV;AAAqD,KAA/D,E,iBAAgD,KAAA,CAAA,O,CAAhD,EAAuD;AAAA;AAAvD,K,EACA,YAAA,CAAkD,IAAlD,EAAkD,IAAlD,EAAkD,CAA9C,YAAA,CAAyC,QAAzC,EAAyC;AAAhC,MAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,eAAE,QAAA,CAAA,YAAA,IAAA,QAAA,CAAA,YAAA,OAAA,QAAA,YAAF;AAAA,OAAA;AAA2B,KAAzC,EAA8B,IAA9B,CAA8C,CAAlD,C,CANF,C;GAOK,CAPL,E;;AAAA,GAQM,EAjBR,C,qCA4CI,EA1BsB,MAAA,CAAA,S,kBAA1B,YAAA,CAyBQ,OAzBR,EAAA,WAAA,EAyBQ,CAxBN,WAwBM,G,kBAbN,YAAA,CAYK,SAZL,EAYK,IAZL,EAYK,WAAA,CAZ+B,KAAA,CAAA,QAY/B,EAZuC,UAAjB,KAAiB,EAAZ;yBAAhC,YAAA,CAYK,IAZL,EAYK;AAZD,MAAA,KAAK,EAAC,SAYL;AAZ0C,MAAA,GAAG,EAAE;AAY/C,KAZL,E,CACE,YAAA,CAA4B,IAA5B,EAA4B,IAA5B,EAA4B,gBAAA,CAArB,KAAK,CAAC,OAAe,CAA5B,EAAoB;AAAA;AAApB,K,EACA,YAAA,CAAuB,IAAvB,EAAuB,IAAvB,EAAuB,gBAAA,CAAhB,KAAK,CAAC,EAAU,CAAvB,EAAe;AAAA;AAAf,K,EACA,YAAA,CAA6B,IAA7B,EAA6B,IAA7B,EAA6B,gBAAA,CAAtB,KAAK,CAAC,QAAgB,CAA7B,EAAqB;AAAA;AAArB,K,EACA,YAAA,CAA2B,IAA3B,EAA2B,IAA3B,EAA2B,gBAAA,CAApB,KAAK,CAAC,MAAc,CAA3B,EAAmB;AAAA;AAAnB,K,EACA,YAAA,CAAyB,IAAzB,EAAyB,IAAzB,EAAyB,gBAAA,CAAlB,KAAK,CAAC,IAAY,CAAzB,EAAiB;AAAA;AAAjB,K,EACA,YAAA,CAAgC,IAAhC,EAAgC,IAAhC,EAAgC,gBAAA,CAAzB,KAAK,CAAC,WAAmB,CAAhC,EAAwB;AAAA;AAAxB,K,EACA,YAAA,CAA2B,IAA3B,EAA2B,IAA3B,EAA2B,gBAAA,CAApB,KAAK,CAAC,MAAc,CAA3B,EAAmB;AAAA;AAAnB,K,EACA,YAAA,CAEK,IAFL,EAEK;AAFA,MAAA,KAAK,EAAA,CAAG,KAAK,CAAC,MAAN,GAAY,CAAZ,GAAY,KAAZ,GAAY,OAAf;AAEL,KAFL,E,iBACK,KAAK,CAAC,M,CADX,EACiB;AAAA;AADjB,K,EAGA,YAAA,CAA+B,IAA/B,EAA+B,IAA/B,EAA+B,gBAAA,CAAxB,KAAK,CAAC,UAAkB,CAA/B,EAAuB;AAAA;AAAvB,K,CAXF,C;GAYK,CAZL,E;;AAAA,GAaM,EAzBR,C,qCA0BI,CA7CN,CA+CI,EADJ,mBAAA,CAAA,2DAAA,CACI,CA1DN,C","sourcesContent":["<template>\r\n  <div id=\"content\">\r\n    <!-- <p id=\"head\" v-if=\"bool\">財務報表 / 代理{{}}的會員 / 會員{{}}的明細</p> -->\r\n    <div id=\"totalBet\" v-if=\"closeGame\">\r\n      <p>總注單數量</p>\r\n      <p>{{ count }}</p>\r\n    </div>\r\n    <div id=\"totalMoney\" v-if=\"closeGame\">\r\n      <p>總下注金額</p>\r\n      <p>{{ amount }}</p>\r\n    </div>\r\n    <div></div>\r\n    <div id=\"tables\">\r\n      <table id=\"firstT\" v-if=\"bool\">\r\n        <tr class=\"title\">\r\n          <th>會員帳號</th>\r\n          <th>姓名</th>\r\n          <th>注單數量</th>\r\n          <th>總下注金額</th>\r\n          <th>總輸贏金額</th>\r\n          <th>會員注單明細</th>\r\n        </tr>\r\n        <tr class=\"trhover\" v-for=\"item in player\" :key=\"item\">\r\n          <td>{{ item.account }}</td>\r\n          <td>{{ item.name }}</td>\r\n          <td>{{ count }}</td>\r\n          <td>{{ amount }}</td>\r\n          <td :class=\"[results > 0 ? 'red' : 'green']\">{{ results }}</td>\r\n          <td><button @click=\"showGameData\">前往</button></td>\r\n        </tr>\r\n      </table>\r\n      <table id=\"secondT\" v-if=\"closeGame\">\r\n        <tr class=\"title\">\r\n          <th>下注時間</th>\r\n          <th>注單編號</th>\r\n          <th>投注館別</th>\r\n          <th>投注項目</th>\r\n          <th>投注金額</th>\r\n          <th>遊戲前金額</th>\r\n          <th>遊戲結果</th>\r\n          <th>會員輸贏</th>\r\n          <th>遊戲後金額</th>\r\n        </tr>\r\n        <tr class=\"trhover\" v-for=\"items in gameData\" :key=\"items\">\r\n          <td>{{ items.betTime }}</td>\r\n          <td>{{ items.id }}</td>\r\n          <td>{{ items.gameType }}</td>\r\n          <td>{{ items.object }}</td>\r\n          <td>{{ items.bets }}</td>\r\n          <td>{{ items.moneyBefore }}</td>\r\n          <td>{{ items.status }}</td>\r\n          <td :class=\"[items.result > 0 ? 'red' : 'green']\">\r\n            {{ items.result }}\r\n          </td>\r\n          <td>{{ items.moneyAfter }}</td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n    <!-- <button v-if=\"closeGame\" @click=\"reset\">重選</button> -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: [\"bool\", \"closeGame\"],\r\n  data() {\r\n    return {\r\n      showPlayerData: false,\r\n      player: [],\r\n      amount: 0,\r\n      count: 0,\r\n      results: [],\r\n      gameData: [],\r\n    };\r\n  },\r\n  watch: {\r\n    bool() {\r\n      // get會員帳號跟名字\r\n      axios\r\n        .get(\"http://127.0.0.1:3001/backend/getMember\")\r\n        .then((e) => {\r\n          console.log(e.data, \"account\");\r\n          this.player = e.data;\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n      // get總下注金額\r\n      axios\r\n        .get(\"http://127.0.0.1:3001/backend/getTotalBets\")\r\n        .then((e) => {\r\n          console.log(e.data, \"amount\");\r\n          this.amount = e.data;\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n      // get注單數量\r\n      axios\r\n        .get(\"http://127.0.0.1:3001/backend/getCount\")\r\n        .then((e) => {\r\n          console.log(e.data, \"count\");\r\n          this.count = e.data;\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n      // get總輸贏金額\r\n      axios\r\n        .get(\"http://127.0.0.1:3001/backend/getResults\")\r\n        .then((e) => {\r\n          console.log(e.data, \"results\");\r\n          this.results = e.data;\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n  },\r\n  methods: {\r\n    // 點擊前往按鈕\r\n    showGameData() {\r\n      // this.showGame = true;\r\n      this.$emit(\"close\", this.showPlayerData);\r\n      // get選取的遊戲\r\n      axios\r\n        .get(\"http://127.0.0.1:3001/backend/fetchGame\")\r\n        .then((e) => {\r\n          console.log(e);\r\n          this.gameData = e.data;\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    reset() {\r\n      window.location.href = \"/\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#content {\r\n  background-color: #545a5f;\r\n  position: absolute;\r\n  display: inline-block;\r\n  top: 9%;\r\n  left: 30%;\r\n  width: 70%;\r\n  height: 91%;\r\n  overflow: auto;\r\n  font-size: 10px;\r\n  color: white;\r\n}\r\n#firstT {\r\n  width: 51.5%;\r\n  background-color: #343a40;\r\n}\r\n#secondT {\r\n  width: 98%;\r\n  background-color: #343a40;\r\n}\r\n#tables {\r\n  /* background-color: #343a40; */\r\n  margin-left: 18px;\r\n  margin-top: 20px;\r\n  /* width: 96%; */\r\n  text-align: center;\r\n  overflow-y: auto;\r\n  height: 67%;\r\n}\r\n#head {\r\n  margin-left: 20px;\r\n  color: rgb(150, 122, 252);\r\n}\r\n#totalBet {\r\n  margin-top: 20px;\r\n  margin-left: 20px;\r\n  background-color: rgb(199, 147, 50);\r\n  width: 150px;\r\n  text-align: center;\r\n  display: inline-block;\r\n  height: 50px;\r\n  line-height: 10px;\r\n}\r\n#totalMoney {\r\n  margin-top: 20px;\r\n  margin-left: 20px;\r\n  background-color: rgb(63, 63, 207);\r\n  width: 150px;\r\n  text-align: center;\r\n  display: inline-block;\r\n  height: 50px;\r\n  line-height: 10px;\r\n}\r\n.title {\r\n  background-color: rgb(113, 90, 197);\r\n}\r\ntable {\r\n  border-collapse: collapse;\r\n  text-align: center;\r\n  width: 100%;\r\n  overflow-x: auto;\r\n  display: block;\r\n  white-space: nowrap;\r\n}\r\n\r\nth,\r\ntd {\r\n  border-collapse: collapse;\r\n  text-align: center;\r\n  border: 1px solid rgb(51, 51, 51);\r\n  padding: 10px;\r\n  margin: 0;\r\n}\r\n\r\n.trhover:hover {\r\n  background-color: rgba(92, 97, 156, 0.493);\r\n}\r\n.green {\r\n  color: green;\r\n}\r\n.red {\r\n  color: red;\r\n}\r\n</style>"],"sourceRoot":""}]}