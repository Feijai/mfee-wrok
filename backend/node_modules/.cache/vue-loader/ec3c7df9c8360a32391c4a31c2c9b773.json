h>\n        <th>遊戲結果</th>\n        <th>會員輸贏</th>\n        <th>遊戲後金額</th>\n      </tr>\n      <tr v-for=\"items in gameData\" :key=\"items\">\n        <td>{{ items.betTime }}</td>\n        <td>{{ items.id }}</td>\n        <td>{{ items.gameType }}</td>\n        <td>{{ items.object }}</td>\n        <td>{{ items.bets }}</td>\n        <td>{{ items.moneyBefore }}</td>\n        <td>{{ items.status }}</td>\n        <td>{{ items.result }}</td>\n        <td>{{ items.moneyAfter }}</td>\n      </tr>\n    </table>\n    <button v-if=\"closeGame\" @click=\"reset\">重選</button>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: [\"bool\", \"closeGame\"],\n  data() {\n    return {\n      showPlayerData: false,\n      player: [],\n      amount: [],\n      count: [],\n      results: [],\n      gameData: [],\n    };\n  },\n  watch: {\n    bool() {\n      // get會員帳號跟名字\n      axios\n        .get(\"http://127.0.0.1:3000/getMember\")\n        .then((e) => {\n          console.log(e.data, \"account\");\n          this.player = e.data;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n      // get總下注金額\n      axios\n        .get(\"http://127.0.0.1:3000/getTotalBets\")\n        .then((e) => {\n          console.log(e.data, \"amount\");\n          this.amount = e.data;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n      // get注單數量\n      axios\n        .get(\"http://127.0.0.1:3000/getCount\")\n        .then((e) => {\n          console.log(e.data, \"count\");\n          this.count = e.data;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n      // get總輸贏金額\n      axios\n        .get(\"http://127.0.0.1:3000/getResults\")\n        .then((e) => {\n          console.log(e.data, \"results\");\n          this.results = e.data;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n  },\n  methods: {\n    // 點擊前往按鈕\n    showGameData() {\n      // this.showGame = true;\n      this.$emit(\"close\", this.showPlayerData);\n      // get選取的遊戲\n      axios\n        .get(\"http://127.0.0.1:3000/fetchGame\")\n        .then((e) => {\n          console.log(e);\n          this.gameData = e.data;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n    reset() {\n      window.location.href = \"/\";\n    },\n  },\n};\n</script>\n\n<style scope>\n#content {\n  background-color: #6c757d0c;\n  position: absolute;\n  display: inline-block;\n  top: 9%;\n  left: 40%;\n  width: 60%;\n  height: 91%;\n  overflow: auto;\n  font-size: 10px;\n  color: white;\n}\ntable,\nth,\ntd {\n  border-collapse: collapse;\n  text-align: center;\n  border: 1px solid white;\n  padding: 10px;\n  margin: 0;\n}\n\ntable {\n  border: 1px solid white;\n  width: 100%;\n}\n\ntr:hover {\n  background-color: rgba(92, 97, 156, 0.493);\n}\n</style>"]}]}